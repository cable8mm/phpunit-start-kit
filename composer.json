{
    "name": "cable8mm/phpunit-start-kit",
    "description": "Phpunit Start Kit is the simple code for starting TDD",
    "keywords": [
        "phpunit",
        "php",
        "development",
        "tdd",
        "startkit"
    ],
    "type": "project",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "ext-intl": "*"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}