{
    "name": "cable8mm/phpunit-start-kit",
    "description": "Phpunit Start Kit is the simple code for starting TDD",
    "keywords": [
        "phpunit",
        "php",
        "development",
        "tdd",
        "startkit"
    ],
    "type": "project",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "laravel/pint": "^1.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Cable8mm\\Example\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\Example\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "start": [
            "@php bin/init.php"
        ],
        "test": [
            "./vendor/bin/phpunit tests"
        ],
        "lint": "./vendor/bin/pint"
    }
}